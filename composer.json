{
  "name": "torvalds/yii2-jquery-slimscroll",
  "description": "Yii2 jquery-slimscroll",
  "keywords": [
    "jquery",
    "slimscroll",
  ],
  "type": "yii2-extension",
  "license": "BSD",
  "support": {
    "source": "https://github.com/torvaldz/yii2-jquery-slimscroll"
  },
  "authors": [
    {
      "name": "Marco Migozzi",
      "email": "torvaldz@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "torvalds\\slimscroll\\": ""
    }
  },
  "minimum-stability": "dev",
  "require": {
    "bower-asset/slimscroll": "*"
  },
  "extra": {
    "asset-installer-paths": {
      "bower-asset-library": "vendor/bower"
    }
  }
}
